{
  "name": "tatakae-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --tsc",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run": "npm run build && npm run typeorm migration:run",
    "typeorm:full:run": "npm run typeorm schema:drop && npm run typeorm:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --clearCache && jest --watchAll --runInBand --config ./jest.config.ts",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@mux/mux-node": "^3.3.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/typeorm": "^8.0.2",
    "@vitalets/google-translate-api": "^7.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "handbrake-js": "^6.0.0",
    "helmet": "^4.6.0",
    "i18next": "^21.3.3",
    "isomorphic-fetch": "^3.0.0",
    "mal-scraper": "^2.11.3",
    "matroska-subtitles": "^3.3.2",
    "morgan": "^1.10.0",
    "node-webvtt": "^1.9.3",
    "parse-torrent": "^9.1.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38",
    "webtorrent": "^1.5.8",
    "zero-fill": "^2.2.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/handbrake-js": "^5.0.1",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^27.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.0.0",
    "@types/parse-torrent": "^5.8.4",
    "@types/supertest": "^2.0.11",
    "@types/webtorrent": "^0.109.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@anime/anime(|/.*)$": "<rootDir>/libs/anime/src/$1",
      "^@torrent/torrent(|/.*)$": "<rootDir>/libs/torrent/src/$1",
      "^@description/description(|/.*)$": "<rootDir>/libs/description/src/$1",
      "^@video/video(|/.*)$": "<rootDir>/libs/video/src/$1",
      "^@shared/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forestus/tatakae-nestjs.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/forestus/tatakae-nestjs/issues"
  },
  "homepage": "https://github.com/forestus/tatakae-nestjs#readme"
}
